{
    "presets": [
        "es2015",
        "stage-2",
        "env",
        "react"
    ],
    "plugins": [
        ["import-glob"],
        ["transform-runtime"],
        ["transform-async-to-generator"],
        ["react-hot-loader/babel"],
        ["transform-class-properties"],
        ["dva-hmr"],
        [
            "import",
            {
              "libraryName": "antd",
              "style": true
            }
        ]
    ]
}
